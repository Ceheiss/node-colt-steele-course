<%include partials/header%>
<h1>The Posts Page</h1>

<h2>Rendered with for loop</h2>
<% for(var i = 0; i < posts.length; i++){ %>
<li><%=posts[i].title%> by - <strong><%=posts[i].author%></strong></li>
<% } %>
<h2>Rendered with for each</h2>
<% posts.forEach(function(post){ %>
<ol>
<li><%=post.title%> by - <strong><%=post.author%></strong></li>
</ol>
<% }) %>
<%include partials/footer%>
